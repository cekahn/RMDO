			"items": {
				"$ref": "RMDO.method.json#"
			}
		},
		"Tasks": {
			"description": "Tasks to which the model is applied, from Papers With Code",
			"$comment": "RMDO:00119",
			"type": "array",
			"items": {
				"$ref": "RMDO.task.json#"
			}
		}
