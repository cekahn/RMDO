	"properties": {
		"Role": {
			"type": "string",
			"enum": [
				"Training",
				"Optimization",
				"Testing",
				"Internal testing",
				"External testing"
			]
		},
		"Subsets": {
			"description": "Subsets of partition by age, sex, and/or race.",
			"$comment": "",
			"type": "array",
			"items": {
				"$ref": "RMDO.subset.json#"
			}
		},
		"Comments": {
			"$comment": "RMDO:00070",
			"type": "string"
		}
	},
	"additionalProperties": false
}